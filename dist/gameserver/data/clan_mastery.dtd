<?xml version="1.0" encoding="utf-8"?>
<!ELEMENT list (mastery)*>

	<!ELEMENT mastery (skill)*>
	<!ATTLIST mastery
		id CDATA #REQUIRED
		clan_level CDATA #REQUIRED
		clan_reputation CDATA #REQUIRED
