			<add stat="attackWater" value="50"/>
			<add stat="attackWind" value="50"/>
			<add stat="attackEarth" value="50"/>
			<add stat="attackHoly" value="50"/>
			<add stat="attackUnholy" value="50"/>
			<add stat="defenceWater" value="#defenceAtt"/>
			<add stat="defenceFire" value="#defenceAtt"/>
			<add stat="defenceWind" value="#defenceAtt"/>
			<add stat="defenceEarth" value="#defenceAtt"/>
			<add stat="defenceHoly" value="#defenceAtt"/>
			<add stat="defenceUnholy" value="#defenceAtt"/>
