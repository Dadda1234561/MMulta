			<add stat="talismansLimit" value="5"/>
			<add order="0x10" stat="STR" value="#STR"/>	<!-- СИЛ(STR) -->
			<add order="0x10" stat="DEX" value="#DEX"/> <!-- ЛВК(DEX) -->
